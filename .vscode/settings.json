{
  "cmake.buildDirectory": "/build/${buildType}",
  "cmake.saveBeforeBuild": true,
  "cmake.generator": "Ninja",
  "cmake.configureArgs": [
    "-DCMAKE_EXPORT_COMPILE_COMMANDS=1"
  ],
  "cmake.options.statusBarVisibility": "compact",
  "cmake.options.advanced.launch.statusBarVisibility": "hidden",
  "cmake.options.advanced.ctest.statusBarVisibility": "hidden",
  "cmake.options.advanced.cpack.statusBarVisibility": "hidden",
  "cmake.options.advanced.kit.statusBarVisibility": "hidden",
  "cmake.options.advanced.workflow.statusBarVisibility": "hidden",
  "uncrustify.configPath.linux": "${workspaceFolder}/etc/uncrustify.cfg",
  "cmakeExplorer.extraCtestRunArgs": "-V -j 1 --timeout 10",
  "cmakeExplorer.suiteDelimiter": "/",
  "cmakeExplorer.testFileVar": "TEST_FILE",
  "[c]": {
    "editor.defaultFormatter": "benjarvis.uncrustify"
  },
  "cmake.ctest.testExplorerIntegrationEnabled": false,
  "cmakeExplorer.parallelJobs": 1,
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "files.associations": {
    "enum_xdr.h": "c",
    "union_xdr.h": "c",
    "uint32_vector_one_xdr.h": "c",
    "uint32_array_xdr.h": "c"
  }
}